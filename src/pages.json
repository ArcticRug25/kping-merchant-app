{
	"pages": [
		//pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTextStyle": "white",
				"navigationBarBackgroundColor": "#ffffff",
				// #ifdef APP-PLUS
				"enablePullDownRefresh": false,
				// #endif
				"app-plus": {
					"scrollIndicator": "none"
				}
			}
		},
		{
			"path": "pages/message/message",
			"style": {
				"navigationBarTitleText": "Message",
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white",
				// #ifdef APP-PLUS
				"enablePullDownRefresh": false,
				// #endif
				"app-plus": {
					"scrollIndicator": "none"
				}
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white",
				"navigationBarBackgroundColor": "#ffffff",
				// #ifdef APP-PLUS
				"enablePullDownRefresh": false,
				// #endif
				"app-plus": {
					"scrollIndicator": "none"
				}
			}
		},
		{
			"path": "pages/login/areaCode",
			"style": {
				"navigationStyle": "custom",
				"app-plus": {
					"animationType": "slide-in-bottom",
					"popGesture": "none"
				}
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "loading...",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#F8F8F8",
		"titleNView": true
	},
	"tabBar": {
		"color": "#515151",
		"selectedColor": "#5064eb",
		"borderStyle": "#f5f5f5",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/index/index",
				"iconPath": "static/images/home-01.png",
				"selectedIconPath": "static/images/home-02.png",
				"text": "Merchant"
			},
			{
				"pagePath": "pages/message/message",
				"iconPath": "static/images/message-01.png",
				"selectedIconPath": "static/images/message-02.png",
				"text": "Message"
			}
		]
	},
	"easycom": {},
	"subPackages": []
}
